<script type="application/javascript">
	document.body.className = "js";
	function detectCSSFeature(feat){
        var feature = false,
        domPrefixes = 'Webkit Moz ms O'.split(' '),
        elm = document.createElement('div'),
        featCapital = null;

        feat = feat.toLowerCase();

        if( elm.style[feat] !== undefined ) { feature = true; }

        if( feature === false ) {
            featCapital = feat.charAt(0).toUpperCase() + feat.substr(1);
            for( var i = 0; i < domPrefixes.length; i++ ) {
                if( elm.style[domPrefixes[i] + featCapital ] !== undefined ) {
                  feature = true;
                  break;
                }
            }
        }
        return feature;
    }
    console.log(detectCSSFeature("transition"));

	document.body.className = "js transition";
</script>
